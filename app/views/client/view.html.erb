<% content_for :topbar do %>
  <%= link_to "view.pdf" do %>
    <i class="fa fa-print"></i>&nbsp;Imprimer la fiche
  <% end %>
<% end %>
<object width="100%"
        style="position:absolute; height:100%; top:45px; z-index: 999; margin-top: 10px"
        type="application/pdf"
        data="view.pdf" id="pdf_content">
  <div style="margin-top: 20px; text-align: center">
    <a target="_blank" href="view.pdf" class="btn btn-primary btn-lg">
      <i class="fa fa-cloud-download"></i>&nbsp;Telecharger la fiche</a>
    <%= link_to "view.pdf", class: "btn btn-primary btn-lg" do %><i class="fa fa-print"></i>&nbsp;Imprimer la fiche<% end %>
  </div>
</object>
